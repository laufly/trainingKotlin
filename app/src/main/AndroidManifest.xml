<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iyunxiao.training">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.iyunxiao.activity.CorountineActivity"></activity>
        <activity android:name="com.example.iyunxiao.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.iyunxiao.activity.RxBusActivity" />
        <activity android:name="com.example.iyunxiao.activity.WebviewActivity" />
        <activity android:name="com.example.iyunxiao.activity.ReplayActivity" />
        <activity android:name="com.example.iyunxiao.activity.CodeSendActivity" />
        <activity android:name="com.example.iyunxiao.activity.ScreenShot2Activity" />
        <activity android:name="com.example.iyunxiao.activity.RecordPlayActivity" />
    </application>

</manifest>